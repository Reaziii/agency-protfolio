] = useState(false);\n  let isMounted = false;\n\n  useEffect(() => {\n    isMounted = true;\n    setDisplay(true);\n    return () => {\n      isMounted = false;\n    };\n  });\n\n  const options = {\n    loop: true,\n    nav: false,\n    dots: false,\n    smartSpeed: 2000,\n    margin: 30,\n    autoplayHoverPause: true,\n    autoplay: true,\n    items: 1,\n  };\n\n  return (\n    <div className=\"section border-top\" dir=\"ltr\">\n      <div className=\"container\">\n        {values.length && display ? (\n          <OwlCarousel className=\"owl-carousel\" {...options}>\n            {values.map((item, key) => (\n              <TestimonialBox\n                key={key}\n                item={item}\n                imgUrl={item.Profile_Picture.url}\n              />\n            ))}\n          </OwlCarousel>\n        ) : null}\n        {/* end owl-carousel */}\n      </div>\n      {/* end container */}\n    </div>\n  );\n};\n\nexport default ClientReviews;\n","/home/reaz/Desktop/react/agency-portfolio/client/src/components/FAQ/FAQ.jsx",["417","418"],"import React from \"react\";\nimport {\n  Accordion,\n  AccordionItem,\n  AccordionItemHeading,\n  AccordionItemPanel,\n  AccordionItemButton,\n} from \"react-accessible-accordion\";\nimport \"react-accessible-accordion/dist/fancy-example.css\";\nimport { connect } from \"react-redux\";\nimport FormattedText from \"../../hooks/FormattedText\";\n\nconst FAQ = ({ values, translation }) => {\n  return (\n    <div className=\"section-lg\">\n      <div className=\"container\">\n        <div className=\"row col-spacing-50\">\n          <div className=\"col-md-6 col-lg-6\">\n            <h2>{ translation === 'Hebrew' ? 'אירוח אתרים' : 'Web Hosting'} </h2>\n            {values.length ? (\n              <Accordion>\n                {values.map((item, key) => {\n                    if(item.Web_Hosting){\n                      return  <AccordionItem key={key}>\n                      <AccordionItemHeading>\n                        <Ac